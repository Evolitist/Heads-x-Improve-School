<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <com.example.homework_3.ListTile
        android:id="@+id/list_tile"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:subtitle="subtitle"
        android:title="Title"
        app:hasCheckbox="true"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0"
        app:leading="@drawable/ic_launcher_background"
        app:visibility_img="visible" />

    <com.example.homework_3.ListTile
        android:id="@+id/list_tile2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:subtitle="subtisdfsdtle"
        android:title="Title"
        app:hasCheckbox="true"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/list_tile" />


</androidx.constraintlayout.widget.ConstraintLayout>