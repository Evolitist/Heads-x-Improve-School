<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.homework5">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Homework5">
        <activity
            android:name=".ui.ServicesListActivity"
            android:theme="@style/Theme.Homework5.ServicesListNoTitleBar" />
        <activity android:name=".ui.FifthActivity" />
        <activity
            android:launchMode="singleTop"
            android:name=".ui.FourthActivity" />
        <activity android:name=".ui.ThirdActivity" />
        <activity android:name=".ui.SecondActivity" />
        <activity
            android:launchMode="singleTask"
            android:name=".ui.FirstActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>