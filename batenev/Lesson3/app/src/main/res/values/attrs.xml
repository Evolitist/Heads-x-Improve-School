<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ListTileView">
        <attr name="title" format="string" />
        <attr name="subtitle" format="string" />
        <attr name="leading" format="reference" />
        <attr name="hasCheckbox" format="boolean" />
    </declare-styleable>

    <declare-styleable name="MyProfileListItem">
        <attr name="hint" format="string" />
        <attr name="data" format="string" />
        <attr name="hasButton" format="boolean" />
        <attr name="buttonPosition" format="enum">
            <enum name="Right" value="0" />
            <enum name="Left" value="1" />
        </attr>
    </declare-styleable>
</resources>